@model IEnumerable<TouristAgency.Models.BookedTour>
<br />
<h2></h2>

    <table class="table" style="margin:10px">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Tour.City)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Number)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    <a href="~/Home/TourPage/@item.TourId">@Html.DisplayFor(modelItem => item.Tour.City)</a>
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Number)
                </td>
            </tr>
        }

    </table>

<script src="~/Scripts/jquery.min.js"></script>
<script src="~/Scripts/jquery.scrollex.min.js"></script>
<script src="~/Scripts/main.js"></script>
<script src="~/Scripts/skel.min.js"></script>
<script src="~/Scripts/util.js"></script>
<script src="~/Scripts/jquery.scrolly.min.js"></script>
